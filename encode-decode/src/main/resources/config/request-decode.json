{
  "$schema" : "http://json-schema.org/draft-07/schema#",
  "type" : "object",
  "required" : [ "enabled", "decoders" ],
  "properties" : {
    "enabled" : {
      "type" : "boolean",
      "description" : "request decode handler for gzip and deflate"
    },
    "decoders" : {
      "type" : "array",
      "default" : [ "gzip", "deflate" ],
      "items" : {
        "type" : "string"
      }
    }
  }
}